<%= form_for(@group) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>

  <%= f.text_area :description, :cols => "30", :rows => "10" %>

  <%= f.submit "Publish", class: "btn btn-large btn-primary" %>

<% end %>